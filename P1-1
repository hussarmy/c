#include <stdio.h>

int main() {
int a;
int b;
int c;

    printf ("enter value of a\n");

    scanf( "%d",&a     );
    printf ("enter vlue of b\n");
    scanf(  "%d", &b       );
    c=a;
    a=b;
    b=c;
    printf("value of a %d",a);
    printf("value of b %d",b);
    return 0;
}
