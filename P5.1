/*write a program to accept the value of year as input from keyword and print whether it is leap year or not*/
#include <stdio.h>
void main() 
{
    int year;
    printf("Enter year:");
    scanf("%d",&year);
        if(year%4==0)
        {
            printf("year is leap year");
        }
        else
        {
            printf("year is not leap year ")
        }
    
    return 0;
    
}
    
