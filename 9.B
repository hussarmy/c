#include <stdio.h>

void main() 
{
    int n;
    int i;
    printf("Enter number  ");
    scanf("%d",&n);
    for  (i=1;i<=10;i++)
     {
        printf("%d*%d=%d\n",n,i,n*i);
    }

    
}

