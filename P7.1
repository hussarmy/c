              break;
        case 5 :
              printf("Friday");
              break;
        case 6 : 
              printf("Saturday");
              break;
        case 7 :       
              printf("Sunday");
              break;
        default :
              printf("Invalid input! Please enter week 1 to 7 .");
         }
 
}
